{
  "$schema": "https://www.raycast.com/schemas/extension.json",
  "name": "spotify_player",
  "title": "Spotify Player",
  "description": "Full Spotify Player for Raycast",
  "icon": "spotify-logo.png",
  "author": "reckoning-dev",
  "version": "1.0.0",
  "license": "MIT",
  "commands": [
    {
      "name": "index",
      "title": "Spotify Player",
      "description": "Main Spotify Menu",
      "mode": "view"
    }
  ],
  "preferences": [
    {
      "name": "clientId",
      "type": "password",
      "required": true,
      "title": "Client ID",
      "description": "Your clientId value from Spotify",
      "placeholder": "Enter your clientId"
    },
    {
      "name": "secret",
      "type": "password",
      "required": true,
      "title": "Client Secret",
      "description": "Your secret value from Spotify",
      "placeholder": "Enter your secret"
    },
    {
      "name": "refreshToken",
      "type": "password",
      "required": true,
      "title": "Refresh Token",
      "description": "Your refresh Token from Spotify",
      "placeholder": "Enter your refresh Token"
    },
